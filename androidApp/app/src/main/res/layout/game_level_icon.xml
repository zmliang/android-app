<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/course_level50_level_path0"
    tools:layout_editor_absoluteX="8dp"
    tools:layout_editor_absoluteY="8dp"
    android:id="@+id/level_container">

    <ImageButton
        android:transitionName="square"
        android:id="@+id/imageButton6"
        android:layout_width="@dimen/course_icon_size"
        android:layout_height="@dimen/course_icon_size"
        android:background="@drawable/group0_1"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="29dp"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp"
        android:layout_marginTop="11dp"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageButton7"
        android:layout_toStartOf="@+id/imageButton7"
        tools:targetApi="lollipop" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="@dimen/course_icon_size"
        android:layout_height="@dimen/course_icon_size"
        android:background="@drawable/group0_2"
        android:layout_marginBottom="40dp"
        android:layout_above="@+id/imageView"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="@dimen/course_icon_size"
        android:layout_height="@dimen/course_icon_size"
        android:background="@drawable/group0_3"
        android:layout_above="@+id/imageView"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_alignStart="@+id/imageView3" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="@dimen/course_icon_size"
        android:layout_height="@dimen/course_icon_size"
        android:layout_marginTop="28dp"
        android:background="@drawable/group0_4"
        android:layout_below="@+id/imageButton8"
        android:layout_alignLeft="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8"
        android:layout_marginLeft="9dp"
        android:layout_marginStart="9dp" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="@dimen/course_icon_size"
        android:layout_height="@dimen/course_icon_size"
        android:layout_below="@+id/imageView"
        android:layout_marginTop="8dp"
        android:layout_toLeftOf="@+id/imageButton9"
        android:layout_toStartOf="@+id/imageButton9"
        android:background="@drawable/group0_5" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="@dimen/course_icon_size"
        android:layout_height="@dimen/course_icon_size"
        android:layout_alignTop="@+id/imageView3"
        android:layout_toLeftOf="@+id/imageButton10"
        android:layout_toStartOf="@+id/imageButton10"
        android:background="@drawable/group0_6" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:background="@drawable/course_level_map0_2"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        app:srcCompat="@drawable/course_level_map0_3" />

</RelativeLayout>

